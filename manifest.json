{
    "manifest_version": 3,
    "name": "Processar XML",
    "version": "1.0",
    "description": "Captura dados de uma p√°gina e processa um XML.",
    "permissions": ["scripting", "activeTab", "tabs"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}
